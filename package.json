{
  "name": "css-used-chrome-ext",
  "version": "2.4.1",
  "description": "A chrome extension to get all css rules used by the selected DOM and its children.",
  "main": "./src/main.js",
  "scripts": {
    "build": "parcel build ./src/main.js --no-source-maps --global getCssUsed -d ./dest/asset/js -o content.js --experimental-scope-hoisting",
    "test": "node ./test/test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/painty/CSS-Used-ChromeExt.git"
  },
  "keywords": [
    "chrome-extension",
    "chrome-devtools",
    "css-usage",
    "css-rules"
  ],
  "author": "Bobscript",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/painty/CSS-Used-ChromeExt/issues"
  },
  "homepage": "https://github.com/painty/CSS-Used-ChromeExt#readme",
  "dependencies": {
    "is-utf8": "^0.2.1",
    "postcss": "^7.0.17",
    "postcss-safe-parser": "^4.0.1",
    "urijs": "^1.19.1"
  },
  "devDependencies": {
    "parcel-bundler": "^1.12.3"
  }
}
